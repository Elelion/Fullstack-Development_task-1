  -var parameter = parameter || {}
  -var normalFont = parameter.normalFont || 'Lorem ipsum dolors sit amet,'
    -normalFont += 'consectetuer adipiscing elit. Aenean commodo ligula eget'
    -normalFont += 'dolor. Aenean massa. Cum sociis natoque penatibus et '
    -normalFont += 'magnis dis parturient montes, nascetur ridiculus mus. '
    -normalFont += 'Donec quam felis, ultricies nec, pellentesque eu, pretium '
    -normalFont += 'quis, sem. Nulla consequat massa quis enum, Donec pede '
    -normalFont += 'justo, fringilla vel, aliquet nec, vukoutate eget, arcu. '
    -normalFont += 'In enim justo, rhoncus ut, imperdiet a, venenatis vitate, '
    -normalFont += 'justo. Nullam dictum felis eu '

  -var boldFont = parameter.boldFont || 'pede mollis pretium. Integer '
