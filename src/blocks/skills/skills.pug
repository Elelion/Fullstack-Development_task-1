mixin skillsList(parameter)
  -var parameter = parameter || {}
  -var creative = parameter.creative || '4'
  -var teamwork = parameter.teamwork || '11'
  -var innovate = parameter.innovate || '8'
  -var communication = parameter.communication || '18'

  section.skills
    +chapter('skills', 6)
    
    .skills__list
      ul.skills__item
        li.skills__point-block
          p.skills__title creative
          .skills__point.skills__point-creative
            +points(creative)

        li.skills__point-block
          p.skills__title teamwork
          .skills__point.skills__point-teamwork
            +points(teamwork)

        li.skills__point-block
          p.skills__title innovate
          .skills__point.skills__point-innovate
            +points(innovate)

        li.skills__point-block
          p.skills__title communication
          .skills__point.skills__point-communication
            +points(communication)